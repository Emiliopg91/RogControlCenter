{
  "name": "RogControlCenter",
  "version": "1.5.6",
  "description": "An utility to manage Asus Rog laptop performance",
  "main": "./out/main/index.js",
  "author": {
    "name": "Emiliopg91",
    "email": "ojosdeserbio@gmail.com",
    "url": "https://electron-react-boilerplate.js.org"
  },
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "concurrently 'pnpm:typecheck:node' 'pnpm:typecheck:web'",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "build": "pnpm lint && pnpm format && pnpm typecheck && electron-vite build",
    "update": "pnpm store prune && pnpm i",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm build && electron-builder --dir",
    "build:linux": "pnpm build && electron-builder --linux"
  },
  "dependencies": {
    "@tser-framework/commons": "^2.0.37",
    "@tser-framework/main": "^2.0.189",
    "@tser-framework/renderer": "2.0.48",
    "adm-zip": "^0.5.16",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "bufferpack": "0.0.6",
    "bufferutil": "^4.0.8",
    "dbus-next": "^0.10.2",
    "follow-redirects": "^1.15.9",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-bootstrap": "2.10.5",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.28.0",
    "systeminformation": "^5.23.5",
    "utf-8-validate": "^6.0.5",
    "uuid": "^11.0.3",
    "ws": "^8.18.0",
    "x11": "^2.3.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@swc/core": "^1.9.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.2",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.80.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dynamic-import": "^1.6.0"
  },
  "extraResources": [
    "./assets/**"
  ],
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
