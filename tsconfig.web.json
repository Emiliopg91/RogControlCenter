{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/commons/**/*",
    "src/renderer/env.d.ts",
    "src/renderer/**/*",
    "src/renderer/**/*.tsx",
    "src/preload/*.d.ts"
, "src/renderer/components", "src/renderer/libraries", "src/renderer/views/main"  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/*"
      ],
      "@commons/*": [
        "src/commons/*"
      ],
      "@resources/*": [
        "resources/*"
      ]
    }
  }
}
